def main(String args) {
    for i : 0..10 {
        print(i)
        if i != 9 {
            print(", ")
        }
    }

    println()
    println(loop(5))
}

def loop(Int n): String {
    var current = n
    while current > 0 {
        print(".")
        current = current - 1
    }
    println()
    return "I 'have' finished!"
}
