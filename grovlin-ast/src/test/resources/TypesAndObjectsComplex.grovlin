type Node {
    def traverse()
}
type Leaf extends Node {
    Int data
    def traverse() {
        print(data)
    }
}
type Tree extends Node {
    Node left
    Node right
    Node parent
    def traverse() {
        left.traverse()
        right.traverse()
    }
}

object BinaryTree as Tree {
    override Node left
    override Node right
    override Node parent
}

object LeafImpl as Leaf {
    override Int data = 1
}

program {
    var tree = BinaryTree()
    tree.left = LeafImpl()
    tree.left.data = 5
    var tree2 = BinaryTree()
    tree.right = tree2
    tree2.left = LeafImpl()
    tree2.right = LeafImpl()
}