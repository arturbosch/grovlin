trait HasData {
    String data
}

object Person as HasData {
    constructor(override String @data,
                Int @age)
}

object Position {

    constructor(Int @startLine
                Int @endLine
                Int @startColumn
                Int @endColumn)
}

trait Node {
    Node parent
    Position position
}

object Tree as Node, HasData {

    constructor(Node @left,
                Node @right,
                override String @data,
                override Node @parent,
                override Position @position) {

        Int @id = rand(10)
    }
}

def main(String args) {
    val p = Person("Artur", 26)
}
